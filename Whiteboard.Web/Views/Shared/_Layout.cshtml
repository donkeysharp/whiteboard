@{
    Layout = "~/Views/Shared/_Master.cshtml";
}

@section SideBar {
    @if (User.IsInRole(Whiteboard.DataAccess.Models.Profile.ROLE_COMMON)) {
        @Html.Partial("SideBars/SideBarCommon")
    } else if (User.IsInRole(Whiteboard.DataAccess.Models.Profile.ROLE_ADMIN)) {
        @Html.Partial("SideBars/SideBarAdmin")
    } else {
        @: There was a problemo jefe
    }
}
@RenderBody()

@section StyleSheets {
    <link href="/Content/css/dashboard.css" rel="stylesheet" />
}
@RenderSection("StyleSheets", required: false)
@RenderSection("Scripts", required: false)