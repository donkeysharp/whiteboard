    <script src="@Url.Content("~/Content/vendor/jquery.min.js")"></script>
    <script src="@Url.Content("~/Content/js/plugins/bootstrap/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/Content/js/plugins/slimscroll/jquery.slimscroll.min.js")"></script>
