<!-- Template dashboard teacher -->
<script type="text/template" id="tpl-teacher-dashboard">
    <h1><%= title %></h1>
    <div>
        <h3><b>Courses: </b> <%= courses %> </h3>
    </div>
    <ul id="teacher-course-list" style="list-style: none"></ul>
</script>

<script type="text/template" id="tpl-teacher-course-item">
    <div class="course-item">
        <img src="<%= PictureUrl %>" />
        <h3><a href="/course/<%= Id %>"><%= Title %></a></h3>
        <p><%= Description %></p>
    </div>
</script>